#include<stdio.h>
int max_sum(int arr[], int n){
    int maxsum=arr[0];
    for (int i=0;i<n;i++){
        int currsum=0;
        for (int j=i;j<n;j++){
            currsum+=arr[j];

            if (currsum>maxsum){
                maxsum=currsum;
            }
        }
    }
    return maxsum;
}
int main() {
    int arr[] = {2, 3, 12, -55, -1, 2, 33};
    int size = sizeof(arr) / sizeof(arr[0]);
    printf("%d", max_sum(arr, size));
    return 0;
}
