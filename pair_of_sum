#include <stdio.h>
#include <stdbool.h>

bool pairofsum(int arr[], int target, int n) {
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {  // j < n, not <= n
            if (arr[i] + arr[j] == target) {
                printf("Pair found: %d + %d = %d\n", arr[i], arr[j], target);
                return true;
            }
        }
    }
    return false; // only return false if no pair is found
}

int main() {
    int arr[] = {1,2,3,4,5,6,7,8,9};
    int n = sizeof(arr) / sizeof(arr[0]);
    int target = 17;

    if (pairofsum(arr, target, n)) {
        printf("Yes, pair exists\n");
    } else {
        printf("No pair found\n");
    }

    return 0;
}
